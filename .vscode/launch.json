{
  "version": "0.1.0",
  "configurations": [
    {
      "name": "Attach to Chrome",
      "type": "chrome",
      "request": "attach",
      "port": 9222,
      "url": "http://localhost:7700",
      "webRoot": "${workspaceFolder}/src",      
      "sourceMaps": true
    },
    {
      "name": "Launch localhost",
      "type": "chrome",
      "request": "launch",
      "url": "http://localhost:7700",
      "webRoot": "${workspaceFolder}/src",
      "sourceMaps": true
    }
  ]
}
